          "apiKeyAuth": {
            "type": "apiKey",
            "in": "header",
            "name": "x-api-key"
          }
        }
      },
      "security": [
        {
          "apiKeyAuth": []
        }
      ]
  },
  "apis": ["**/*.ts"]
