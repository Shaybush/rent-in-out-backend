          "version": "1.0.0",
          "title": "Rent In Out REST API",
          "description": "A REST API built with Express and MongoDB."
      },
      "basePath": "/swagger",
      "tags": [
          { "name": "Auth operations" },
          { "name": "Categories operations" },
          { "name": "Cloudinary operations" },
          { "name": "Posts operations" },
          { "name": "Users operations" }
